{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "target": "ES6",
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    ".next/dev/types/**/*.ts"
, "app/layout.jsx", "components/ui/card.jsx", "components/ui/calendar.jsx", "components/ui/button.jsx", "components/ui/button-group.jsx", "components/ui/breadcrumb.jsx", "components/ui/badge.jsx", "components/ui/avatar.jsx", "components/ui/aspect-ratio.jsx", "components/ui/alert.jsx", "components/ui/alert-dialog.jsx", "components/ui/accordion.jsx", "lib/utils.js", "components/theme-provider.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
